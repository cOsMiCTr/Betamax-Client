;\nimport './genre-view.scss';\nimport {Container, Card, Row, Button} from 'react-bootstrap';\n\n\nexport class GenreView extends React.Component {\n\n    render() {\n        const { genre, movies } = this.props;\n\n        return (\n            <Container fluid>\n                <Card>\n                    <Card.Body>\n                        <Card.Title style={{ textAlign: \"center\", fontSize: \"36px\", margin:\"1rem\"}}>Genre</Card.Title>\n                        <Card.Text>\n                            <span className=\"label\">Name: </span>\n                            <span className=\"value\">{genre.Name}</span>\n                        </Card.Text>\n                        <Card.Text>\n                            <span className=\"label\">Description: </span>\n                            <span className=\"value\">{genre.Description}</span>\n                        </Card.Text>\n\n                        <Button\n                    label=\"Back\"\n                    onClick={() => {\n                      history.back();\n                    }}\n                    style={{ textAlign: \"center\" }}\n                  >Back</Button>\n                    </Card.Body>\n                </Card>\n                <Row style={{ textAlign: \"center\", fontSize: \"36px\", margin:\"2rem\", display:\"grid\"}}>\n                    {movies.map(movie => (\n                        <Card className=\"favorite-movie card-content\" key={movie._id} style={{ margin:\"2rem\"}}>\n                            <Card.Img\n                                className=\"fav-poster\"\n                                variant=\"top\"\n                                src={movie.ImageURL} />\n                            <Card.Body>\n                                <Card.Title className=\"movie_title\">\n                                    {movie.Title}\n                                </Card.Title>\n                            </Card.Body>\n                        </Card>\n                   